<?xml version="1.0" encoding="UTF-8"?>
<a:authorAction xmlns:a="http://www.oxygenxml.com/ns/author/external-action"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.oxygenxml.com/ns/author/external-action http://www.oxygenxml.com/ns/author/external-action/authorAction.xsd"
  id="delBibl">
  <a:name>${i18n(delBibl)}</a:name>
  <a:description>${i18n(delBibl)}</a:description>
  <a:operations>
    <a:operation id="DeleteElementOperation">
      <a:xpathCondition>parent::taxonomy or self::taxonmy</a:xpathCondition>
      <a:arguments>
        <a:argument name="elementLocation">self::bibl</a:argument>
      </a:arguments>
    </a:operation>
  </a:operations>
  <a:enabledInReadOnlyContext>false</a:enabledInReadOnlyContext>
</a:authorAction>
